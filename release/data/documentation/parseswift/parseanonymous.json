    },
    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Anonymous users have some unique characteristics:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Anonymous users do not need a user name or password.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Once logged out, an anonymous user cannot be recovered.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "When the current user is anonymous, the following methods can be used to switch",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "to a different user or convert the anonymous user into a regular one:",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                },
                {
                  "items" : [
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "inlineContent" : [
                                {
                                  "text" : "signup",
                                  "type" : "text"
                                }
                              ],
                              "type" : "emphasis"
                            },
                            {
                              "text" : " converts an anonymous user to a standard user with the given username and password.",
                              "type" : "text"
                            },
                            {
                              "text" : " ",
                              "type" : "text"
                            },
                            {
                              "text" : "Data associated with the anonymous user is retained.",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "inlineContent" : [
                                {
                                  "text" : "login",
                                  "type" : "text"
                                }
                              ],
                              "type" : "emphasis"
                            },
                            {
                              "text" : " switches users without converting the anonymous user.",
                              "type" : "text"
                            },
                            {
                              "text" : " ",
                              "type" : "text"
                            },
                            {
                              "text" : "Data associated with the anonymous user will be lost.",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "Service ",
                              "type" : "text"
                            },
                            {
                              "inlineContent" : [
                                {
                                  "text" : "login",
                                  "type" : "text"
                                }
                              ],
                              "type" : "emphasis"
                            },
                            {
                              "text" : " (e.g. Apple, Facebook, Twitter) will attempt to convert",
                              "type" : "text"
                            },
                            {
                              "text" : " ",
                              "type" : "text"
                            },
                            {
                              "text" : "the anonymous user into a standard user by linking it to the service.",
                              "type" : "text"
                            },
                            {
                              "text" : " ",
                              "type" : "text"
                            },
                            {
                              "text" : "If a user already exists that is linked to the service, it will instead switch to the existing user.",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "Service linking (e.g. Apple, Facebook, Twitter) will convert the anonymous user",
                              "type" : "text"
                            },
                            {
                              "text" : " ",
                              "type" : "text"
                            },
                            {
                              "text" : "into a standard user by linking it to the service.",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    }
                  ],
                  "type" : "unorderedList"
                }
              ]
            }
